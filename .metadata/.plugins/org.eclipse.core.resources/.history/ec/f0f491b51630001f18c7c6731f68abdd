import juego.*
import player.*
import wollok.game.*

test "testX" {
	const jugador = new CabezaDeSnake(image = "cabezaDerecha.png")
	const parte1 = new ParteDeSnake(position = game.at(7,8))
	const colaSnake = new Cola(image = "cola.png", position = game.at(6, 8))
	lasPartesDeSnake.add(jugador)
	lasPartesDeSnake.add(parte1)
	lasPartesDeSnake.add(colaSnake)
	assert.equals(3, lasPartesDeSnake.size())

}