
import wollok.game.*
import juego.*
import player.*

const dondeAparecer = []

class ItemEnNivel{
	var position = game.origin()
	
	method reubicar(){
		dondeAparecer.add(position)
		position = dondeAparecer.anyOne()
		dondeAparecer.remove(position)
	}
}

class Manzana inherits ItemEnNivel {
	var property image = "manzana.png"
	
	method comer() {
		game.removeVisual(image)
		//game.addVisualIn(image, self.ubicacionApropiada())
	}
	
	method ubicacionApropiada() {
		
	}
}